{
  "expo": {
    "name": "candy-crushed",
    "slug": "candy-crushed",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/appicon.png",
    "scheme": "candycrushed",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/appicon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.fero11.candycrushed"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "c395441c-288c-4cc3-b763-53d3226aedea"
      }
    }
  }
}
